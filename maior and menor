programa
{
	
	funcao inicio()
	{
	
  
   real numero1
   real numero2
   real maior
 

   escreva("Digite o primeiro número: ")
   leia(numero1)
   
   escreva("Digite o segundo número: ")
   leia(numero2)
   
   se(numero1 > numero2){
maior = numero1
		}senao{
maior = numero2
		}  
      
  
   
   escreva("O maior número é: ", maior)
   


	}
}
